{
  "name": "ITAEMTdeisngTemplate",
  "version": "0.7.7",
  "description": "后端管理系统模板",
  "scripts": {
    "build": "vite build --mode release",
    "build:site": "vue-tsc --noEmit && vite build --mode site",
    "build:test": "vite build --mode test",
    "dev": "vite --open --mode development",
    "dev:linux": "vite --mode development",
    "dev:mock": "vite --open --mode mock",
    "lint": "eslint --ext .vue,.js,.jsx,.ts,.tsx ./ --max-warnings 0",
    "lint:fix": "eslint --ext .vue,.js,jsx,.ts,.tsx ./ --max-warnings 0 --fix",
    "openapi": "ts-node ./config/openapi.config.ts",
    "prepare": "node -e \"if(require('fs').existsSync('.git')){process.exit(1)}\" || is-ci || husky install",
    "preview": "vite preview",
    "site:preview": "npm run build && cp -r dist _site",
    "stylelint": "stylelint src/**/*.{html,vue,sass,less}",
    "stylelint:fix": "stylelint --fix src/**/*.{html,vue,vss,sass,less}",
    "test": "echo \"no test specified,work in process\"",
    "test:coverage": "echo \"no test:coverage specified,work in process\""
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [],
    "*.{html,vue,vss,sass,less}": []
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "axios": "^1.4.0",
    "dayjs": "^1.11.8",
    "echarts": "^5.4.2",
    "lodash": "^4.17.21",
    "nanoid": "^5.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.33",
    "pinia-plugin-persistedstate": "^3.1.0",
    "qrcode.vue": "^3.3.4",
    "qs": "^6.11.1",
    "shortid": "^2.2.16",
    "tdesign-icons-vue-next": "^0.1.11",
    "tdesign-vue-next": "^1.3.8",
    "tim-js-sdk": "^2.27.5",
    "ts-md5": "^1.3.1",
    "tvision-color": "^1.6.0",
    "umi-request": "^1.4.0",
    "uuid": "^9.0.0",
    "vue": "^3.3.4",
    "vue-clipboard3": "^2.0.0",
    "vue-cookies": "^1.8.3",
    "vue-router": "~4.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@types/echarts": "^4.9.16",
    "@types/lodash": "^4.14.191",
    "@types/node": "^20.3.3",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "@umijs/openapi": "^1.8.3",
    "@vitejs/plugin-vue": "^4.2.3 ",
    "@vitejs/plugin-vue-jsx": "^3.0.1 ",
    "@vue/compiler-sfc": "^3.2.47",
    "@vue/eslint-config-typescript": "^11.0.2",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.30.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-vue": "^9.8.0",
    "eslint-plugin-vue-scoped-css": "^2.2.0",
    "husky": "^8.0.2",
    "less": "^4.1.3",
    "lint-staged": "^13.1.0",
    "mockjs": "^1.1.0",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "prettier": "^2.8.1",
    "stylelint": "~14.9.1",
    "stylelint-config-prettier": "~9.0.4",
    "stylelint-less": "1.0.6",
    "stylelint-order": "~6.0.1",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.5",
    "vite": "^4.3.9",
    "vite-plugin-mock": "^2.9.6",
    "vite-svg-loader": "^4.0.0",
    "vue-tsc": "^1.8.2"
  }
}
